			<column name="class_prac" length="10">
				<comment></comment>
			</column>
		</property>
		<property name="subLCode" type="string">
			<column name="sub_l_code" length="10">
				<comment></comment>
			</column>
		</property>
		<property name="batchNo" type="string">
			<column name="batch_no" length="10">
				<comment></comment>
			</column>
		</property>
		<property name="classHeld" type="big_decimal">
			<column name="class_held" precision="2">
				<comment></comment>
			</column>
		</property>
		<property name="elective" type="string">
			<column name="elective" length="15">
				<comment></comment>
			</column>
		</property>
		<property name="academicYear" type="java.lang.Integer">
			<column name="academic_year" length="4">
				<comment></comment>
			</column>
		</property>
		</class>
</hibernate-mapping>
