<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kp.cms.bo.exam.SpecialFeesBO" table="special_fees">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		<many-to-one name="classes" class="com.kp.cms.bo.admin.Classes" fetch="select">
			<column name="class_id">
				<comment></comment>
			</column>
		</many-to-one>
		<property name="createdBy" type="string" update="false">
			<column name="created_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy" type="string">
			<column name="modified_by" length="50">
				<comment></comment>
			</column>
		</property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.Boolean">
            <column name="is_active">
                <comment></comment>
            </column>
        </property>
		 <property name="tutionFees" type="java.math.BigDecimal">
            <column name="tution_fees" />
        </property>
        <property name="specialFees" type="java.math.BigDecimal">
            <column name="special_fees" />
        </property>
        <property name="applicationFees" type="java.math.BigDecimal">
            <column name="application_fees" />
        </property>
        <property name="lateFineFees" type="java.math.BigDecimal">
            <column name="late_fine_fees" />
        </property>
       
         <property name="academicYear" >
            <column name="academic_year" />
        </property>
        
	</class>
</hibernate-mapping>
