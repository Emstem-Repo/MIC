<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.kp.cms.bo.admission.DemandSlipInstruction" table="demand_slip_instruction">
		<comment></comment>
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
       <many-to-one name="course" class="com.kp.cms.bo.admin.Course" fetch="select" >
            <column name="course_id">
                <comment></comment>
            </column>
        </many-to-one>
        <property name="instruction" type="string">
            <column name="instruction" length="500">
                <comment></comment>
            </column>
        </property>
        <property name="schemeNo" type="java.lang.Integer">
            <column name="scheme_no">
                <comment></comment>
            </column>
        </property>
        <property name="createdBy" type="string" update="false">
            <column name="created_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedBy" type="string">
            <column name="modified_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.Boolean">
            <column name="is_active">
                <comment></comment>
            </column>
        </property>
       
	</class>
</hibernate-mapping>
