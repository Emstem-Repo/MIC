<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kp.cms.bo.exam.ExamRevaluationApplicationNew" table="exam_revaluation_application_new">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		 <many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select">
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
		 <many-to-one name="exam" class="com.kp.cms.bo.exam.ExamDefinitionBO" fetch="select">
            <column name="exam_id">
                <comment></comment>
            </column>
        </many-to-one>
		<many-to-one name="classes" class="com.kp.cms.bo.admin.Classes" fetch="select">
            <column name="class_id">
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="subject" class="com.kp.cms.bo.admin.Subject" fetch="select">
            <column name="subject_id">
                <comment></comment>
            </column>
        </many-to-one>
        
        <many-to-one name="revaluationPgiDetails" class="com.kp.cms.bo.exam.RevaluationApplicationPGIDetails" fetch="select">
            <column name="rev_pgi_id">
                <comment></comment>
            </column>
        </many-to-one>
        
       
		<property name="isApplied" type="boolean">
			<column name="is_applied"></column>
		</property>
		<property name="isRevaluation" type="boolean">
			<column name="is_revaluation"></column>
		</property>
		<property name="isScrutiny" type="boolean">
			<column name="is_scrutiny"></column>
		</property>
		<property name="isAnswerScrptCopy" type="boolean">
			<column name="is_answerscript_copy"></column>
		</property>
		
		<property name="marks" type="string">
			<column name="marks" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="maxMarks" type="string">
			<column name="theory_max_marks" length="50">
				<comment></comment>
			</column>
		</property>
		 <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
		<property name="createdBy" type="string" update="false">
			<column name="created_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy" type="string">
			<column name="modified_by" length="50">
				<comment></comment>
			</column>
		</property>
		
	</class>
</hibernate-mapping>