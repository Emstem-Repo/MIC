<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kp.cms.bo.studentExtentionActivity.StudentExtentionActivityDetails" table="student_extention_activity_details">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		<many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select">
			<column name="student_id">
				<comment></comment>
			</column>
		</many-to-one>
		<many-to-one name="studentExtention" class="com.kp.cms.bo.studentExtentionActivity.StudentExtention" fetch="select">
			<column name="student_extension_id">
				<comment></comment>
			</column>
		</many-to-one>

		<property name="preference"
				  column="preference" 
				  type="int"/>
		
		<property name="createdBy"
				  column="created_by" 
				  type="java.lang.String"
				  update="false"/>
				  
		<property name="createdDate"
				  column="created_date" 
				  type="java.sql.Timestamp"
				  update="false"/>
				  
		<property name="modifiedBy"
				  column="modified_by" 
				  type="java.lang.String"/>
				  
		<property name="lastModifiedDate"
				  column="last_modified_date" 
				  type="java.sql.Timestamp"/>
	</class>
</hibernate-mapping>