<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.kp.cms.bo.studentLogin.StudentCarPass" table="student_carpass">
        <comment></comment>
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="academicYear" type="string">
            <column name="academic_year" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="modelOfVehicle" type="string">
            <column name="model_of_vehicle" length="30">
                <comment></comment>
            </column>
        </property>
        <property name="vehicleNumber" type="string">
            <column name="vehicle_number" length="30">
                <comment></comment>
            </column>
        </property>
        <property name="emergencyContactNo" type="string">
            <column name="emergency_contact_number" length="10">
                <comment></comment>
            </column>
        </property>
        <property name="createdBy" type="string" update="false">
            <column name="created_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedBy" type="string">
            <column name="modified_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.Boolean">
            <column name="is_active">
                <comment></comment>
            </column>
        </property>
         <many-to-one name="studentId" class="com.kp.cms.bo.admin.Student" fetch="select" >
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
        <property name="carPassNo" type="integer">
            <column name="car_pass_number">
                <comment></comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
