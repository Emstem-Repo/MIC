<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kp.cms.bo.studentExtentionActivity.StudentExtention" table="student_extention">
		
		<id name="id" column="id" type="java.lang.Integer">
			 <generator class="native"/>
      	</id>
		
		<property name="activityName"
				  column="text_area" 
				  type="java.lang.String"/>
        
        <property name="displayOrder"
				  column="display_order" 
				  type="java.lang.Integer"/>
				  
		<many-to-one name="studentGroup" class="com.kp.cms.bo.studentExtentionActivity.StudentGroup"
			fetch="select">
			<column name="student_group_id">
				<comment></comment>
			</column>
		</many-to-one>		  			
		
		<property name="createdBy"
				  column="created_by" 
				  type="java.lang.String"
				  update="false"/>
				  
		<property name="createdDate"
				  column="created_date" 
				  type="java.sql.Timestamp"
				  update="false"/>
				  
		<property name="modifiedBy"
				  column="modified_by" 
				  type="java.lang.String"/>
				  
		<property name="lastModifiedDate"
				  column="last_modified_date" 
				  type="java.sql.Timestamp"/>
				  
		<property name="isActive"
				  column="is_active" 
				  type="java.lang.Boolean"/>
		
	</class>
</hibernate-mapping>	