<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kp.cms.bo.exam.ExamSupplementaryApplication" table="exam_supplementary_app">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		 <many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select">
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
		 <many-to-one name="exam" class="com.kp.cms.bo.exam.ExamDefinitionBO" fetch="select">
            <column name="exam_id">
                <comment></comment>
            </column>
        </many-to-one>
		<many-to-one name="classes" class="com.kp.cms.bo.admin.Classes" fetch="select">
            <column name="class_id">
                <comment></comment>
            </column>
        </many-to-one>
        
		<property name="isApplied" type="boolean">
			<column name="is_applied"></column>
		</property>
	
		 <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
		<property name="createdBy" type="string" update="false">
			<column name="created_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy" type="string">
			<column name="modified_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="challanNo" type="string">
			<column name="challan_no" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="challanVerified" type="boolean">
			<column name="challan_verified"></column>
		</property>
		<property name="mode" type="string">
			<column name="mode"></column>
		</property>
		<property name="amount" type="string">
			<column name="amount"></column>
		</property>
	</class>
</hibernate-mapping>