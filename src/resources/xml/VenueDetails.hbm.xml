<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
<class name="com.kp.cms.bo.auditorium.VenueDetails" table="audi_venue_details">
        <comment></comment>
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
       <property name="createdBy" type="string" update="false">
			<column name="created_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy" type="string">
			<column name="modified_by" length="50">
				<comment></comment>
			</column>
		</property>
         <many-to-one name="block" class="com.kp.cms.bo.auditorium.BlockDetails" fetch="select" >
            <column name="block_id">
                <comment></comment>
            </column>
        </many-to-one>
        <property name="venueName" type="string">
            <column name="venue_name" length="30">
                <comment></comment>
            </column>
        </property>
        <property name="roomNo" type="string">
            <column name="room_no" length="15">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.Boolean">
            <column name="is_active">
                <comment></comment>
            </column>
        </property>
        <property name="emailId" type="string">
            <column name="email_id" length="50">
                <comment></comment>
            </column>
        </property>
         <property name="facilityAvailable" type="string">
            <column name="facilities_available" length="500">
                <comment></comment>
            </column>
        </property>
    </class>
</hibernate-mapping>
