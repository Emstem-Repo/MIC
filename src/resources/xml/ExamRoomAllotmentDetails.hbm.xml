<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.kp.cms.bo.examallotment.ExamRoomAllotmentDetails" table="EXAM_room_allotment_details" >
        <comment></comment>
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <many-to-one name="allotment" class="com.kp.cms.bo.examallotment.ExamRoomAllotment" fetch="select">
            <column name="exam_room_allotment_id">
                <comment></comment>
            </column>
        </many-to-one>
		 <many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select">
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
        <many-to-one name="classes" class="com.kp.cms.bo.admin.Classes" fetch="select">
            <column name="class_id">
                <comment></comment>
            </column>
        </many-to-one>
        <property name="columnNO" type="int">
            <column name="column_no">
                <comment></comment>
            </column>
        </property>
         <property name="rowNO" type="int">
            <column name="row_no">
                <comment></comment>
            </column>
        </property>
        <property name="seatingPosition" type="int">
            <column name="seating_position">
                <comment></comment>
            </column>
        </property>
        <property name="createdBy" type="string" update="false">
            <column name="created_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="modifiedBy" type="string">
            <column name="modified_by" length="50">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="isActive" type="java.lang.Boolean">
            <column name="is_active">
                <comment></comment>
            </column>
        </property>
        <many-to-one name="subject" class="com.kp.cms.bo.admin.Subject" fetch="select">
            <column name="subject_id">
                <comment></comment>
            </column>
        </many-to-one>
      </class>
</hibernate-mapping>
