<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.kp.cms.bo.exam.StudentSubjectGroupHistory" table="EXAM_student_sub_grp_history">
		<comment></comment>
		<id name="id" type="int">
			<column name="id" />
			<generator class="native" />
		</id>
		 <many-to-one name="student" class="com.kp.cms.bo.admin.Student" fetch="select">
            <column name="student_id">
                <comment></comment>
            </column>
        </many-to-one>
		 <many-to-one name="subjectGroup" class="com.kp.cms.bo.admin.SubjectGroup" fetch="select">
            <column name="subject_group_id">
                <comment></comment>
            </column>
        </many-to-one>
	
		<property name="schemeNo" type="java.lang.Integer">
			<column name="scheme_no">
				<comment></comment>
			</column>
		</property>
		<property name="createdBy" type="string" update="false">
			<column name="created_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="modifiedBy" type="string">
			<column name="modified_by" length="50">
				<comment></comment>
			</column>
		</property>
		<property name="createdDate" type="timestamp" update="false">
            <column name="created_date" length="0">
                <comment></comment>
            </column>
        </property>
        <property name="lastModifiedDate" type="timestamp">
            <column name="last_modified_date" length="0">
                <comment></comment>
            </column>
        </property>
		
	</class>
</hibernate-mapping>