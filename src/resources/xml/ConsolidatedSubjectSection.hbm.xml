<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.kp.cms.bo.exam.ConsolidatedSubjectSection" table="consolidated_subject_section">
		
		<id name="id" column="id" type="java.lang.Integer">
			 <generator class="native"/>
      	</id>
		
		<property name="sectionName"
				  column="section_name" 
				  type="java.lang.String"/>
				  
		<property name="sectionOrder"
				  column="section_order" 
				  type="java.lang.Integer"/>
				  
		<property name="createdBy"
				  column="created_by" 
				  type="java.lang.String"
				  update="false"/>
				  
		<property name="createdDate"
				  column="created_date" 
				  type="java.sql.Timestamp"
				  update="false"/>
				  
		<property name="modifiedBy"
				  column="modified_by" 
				  type="java.lang.String"/>
				  
		<property name="lastModifiedDate"
				  column="last_modified_date" 
				  type="java.sql.Timestamp"/>
				  
		<property name="isActive"
				  column="is_active" 
				  type="java.lang.Boolean"/>
				  
		<property name="showRespectiveStreams"
				  column="show_respective_streams" 
				  type="java.lang.Boolean"/>
		
	</class>
</hibernate-mapping>	